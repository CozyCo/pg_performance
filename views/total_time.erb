<header role="banner">Total Time</header>
<%= erb :menu %>
<table class="total-time" >
  <thead>
    <tr>
      <th class="total-time">Total (s)</th>
      <th>Mean (ms)</th>
      <th>Calls</th>
      <th>SD (ms)</th>
      <th>Query</th>
    </tr>
  </thead>
  <% total_time_rows.each do |row| %>
      <tr>
        <td><%= (row[:total_time].to_f / 1000).to_i %></td>
        <td><%= (row[:mean_time].to_f).round(2) %></td>
        <td><%= row[:calls].to_i %></td>
        <td><%= (row[:stddev_time].to_f).round(2) %></td>
        <td><%= sql(row[:query]) %></td>
      </tr>
<% end %>
</table>
